{
  "name": "weconnect-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "ionicons": "^4.0.0",
    "node-sass-chokidar": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar ./src/styles/app.scss ./src/styles/app.css --include-path ./node_modules src/styles -o src/styles --output-style compressed",
    "watch-css": "node-sass-chokidar ./src/styles/app.scss ./src/styles/app.css --include-path ./node_modules src/styles -o src/styles --watch --recursive",
    "test": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/*** --collectCoverageFrom=!src/registerServiceWorker.js",
    "test:ci": "react-scripts test --env=jsdom --coverage --collectCoverageFrom=src/*** --collectCoverageFrom=!src/registerServiceWorker.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "lint": "./node_modules/eslint/bin/eslint.js ./src"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^8.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "mock-local-storage": "^1.0.5",
    "prettier-eslint": "^8.8.1"
  }
}
